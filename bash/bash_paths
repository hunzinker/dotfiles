export EDITOR=vim

HEROKU=/usr/local/heroku/bin
BIN=$HOME/bin
COREUTILS=/usr/local/opt/coreutils/libexec/gnubin

export ANDROID_HOME=/usr/local/opt/android-sdk
export RBENV_ROOT=/usr/local/var/rbenv
export JRUBY_OPTS=-J-Xmx4g
export JAVA_HOME=$(/usr/libexec/java_home)
export NVM_DIR=~/.nvm

export PATH=/usr/local/bin:/usr/local/sbin:$COREUTILS:$PATH:$HEROKU:$BIN


MAN_GNUSED=/usr/local/opt/gnu-sed/libexec/gnuman
MAN_COREUTILS=/usr/local/opt/coreutils/libexec/gnuman

export MANPATH=$MAN_COREUTILS:$MAN_GNUSED:$MANPATH
