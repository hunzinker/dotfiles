export EDITOR=vim

export ANDROID_HOME=/usr/local/opt/android-sdk

PYTHON=/usr/local/Cellar/python/2.7.6/bin
HEROKU=/usr/local/heroku/bin
RVM=$HOME/.rvm/bin
BIN=$HOME/bin
COREUTILS=/usr/local/opt/coreutils/libexec/gnubin

export VERSIONER_PYTHON_VERSION=2.7

export PATH=/usr/local/bin:$COREUTILS:$PYTHON:$PATH:$HEROKU:$RVM:$BIN


MAN_COREUTILS=/usr/local/opt/coreutils/libexec/gnuman

export MANPATH=$MAN_COREUTILS:$MANPATH
