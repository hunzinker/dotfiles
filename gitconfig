[user]
    name = %NAME%
    email = %EMAIL%
[core]
    autocrlf = input
[push]
    default = current
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    los = log --color --graph --decorate --oneline
    lol = log --color --graph --decorate
    lols = log --color --graph --decorate --stat
    st = status
    br = branch
    co = checkout
[pull]
    rebase = true
